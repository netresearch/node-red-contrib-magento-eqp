	"devDependencies": {
		"@types/node": "^20.0.0",
		"@types/node-red": "0.20.7",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.0.0",
		"eslint-plugin-html": "^8.0.0",
		"typescript": "^5.1.6"
	},
	"engines": {
		"node": ">=12"
	}
